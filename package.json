{
  "name": "omninode-frontend",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!service-portal --filter=!desk",
    "dev": "turbo run dev --filter=!service-portal --filter=!desk",
    "start": "turbo run start --filter=!service-portal --filter=!desk",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18"
  }
}
