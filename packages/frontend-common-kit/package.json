    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@repo/lib-api-client": "workspace:*",
    "@scaleits-solutions-gmbh/omninode-lib-global-common-kit": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
