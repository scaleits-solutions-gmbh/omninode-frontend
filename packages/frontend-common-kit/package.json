    "@types/node": "^24.8.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "eslint": "^9.38.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "5.9.3"
  }
