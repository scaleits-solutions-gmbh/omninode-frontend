{
    "name": "@repo/lib-frontend-common-kit",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist/**"
    ],
    "scripts": {
        "lint": "eslint . --max-warnings 0",
        "check-types": "tsc --noEmit",
        "build": "tsc -p tsconfig.json",
        "clean": "rimraf dist",
        "prepare": "pnpm run build"
    },
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "@types/node": "^22.15.3",
        "eslint": "^9.33.0",
        "rimraf": "^6.0.0",
        "typescript": "5.9.2"
    }
}